public static String hackerrankInString(String s) {
    // Write your code here
        String target = "hackerrank";
        int index = 0;

        for (int i = 0; i < s.length() && index < target.length(); i++) {
            if (s.charAt(i) == target.charAt(index)) {
                index++;
            }
        }

        if (index == target.length()) {
            return "YES";
        }
        else {
            return "NO";
        }
    }
